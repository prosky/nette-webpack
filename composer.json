{
  "name": "prosky/nette-webpack",
  "description": "Webpack Bridge for Nette Framework",
  "type": "project",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Jan Proskovec"
    }
  ],
  "require": {
    "php": ">=7.2",
    "latte/latte": "^2.5",
    "nette/caching": "^3.0",
    "nette/di": "^3.0",
    "nette/http": "^3.0",
    "nette/schema": "^1.0",
    "nette/utils": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Prosky\\NetteWebpack\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
